{
    "name": "michielroos/typo3migrate",
    "description": "TYPO3 Migration Tools",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Michiel Roos",
            "email": "michiel@michielroos.com"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "symfony/process": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/console": "^3.4",
        "symfony/debug": "^3.4",
        "symfony/filesystem": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "MichielRoos\\TYPO3Migrate\\": "src/TYPO3Migrate",
            "TYPO3\\CMS\\Core\\Utility\\": "src/TYPO3/Utility",
            "TYPO3\\CMS\\Core\\Localization\\Parser\\": "src/TYPO3/Parser"
        }
    },
    "bin": ["bin/typo3migrate"],
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "roave/security-advisories": "dev-master"
    }
}
